    <script>
        window.onload = function () {
            const head = document.getElementsByTagName('head')[0];
            const images = document.querySelectorAll('[data-img]');
            const gifs = document.querySelectorAll('[data-style]');
            const scripts = document.querySelectorAll('[data-src]');
            const maps = document.getElementById('maps');

            const cssSheet = document.createElement('link');
            cssSheet.rel = 'stylesheet';
            cssSheet.href = '/assets/css/index.css';

            head.appendChild(cssSheet);

            setTimeout(() => {
                document.body.removeChild(
                    document.getElementById('load')
                )

                Array.from(scripts).forEach((scriptInfo) => {
                    const scriptEl = document.createElement('script');
                    scriptEl.src = scriptInfo.dataset.src;
                    scriptEl.id = scriptInfo.id;

                    document.body.removeChild(scriptInfo);
                    document.body.appendChild(scriptEl);
                });

                Array.from(images).forEach((imagesInfo) => {
                    const imgEl = document.createElement('img');
                    imgEl.src = imagesInfo.dataset.img;
                    imgEl.alt = imagesInfo.dataset.alt;

                    imagesInfo.parentNode.prepend(imgEl);
                    imagesInfo.parentNode.removeChild(imagesInfo);
                });

                Array.from(gifs).forEach((gif) => {
                    gif.style = gif.dataset.style;
                });

                const iframe = document.createElement('iframe');
                iframe.src = 'https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3704.656070137298!2d-48.21210378550604!3d-21.793568104572586!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x94b8f1887a4c9a99%3A0xd2b619efd96feb76!2sAv.+Romulo+Lupo%2C+1487+-+Jardim+Universal%2C+Araraquara+-+SP%2C+14801-700!5e0!3m2!1sen!2sbr!4v1561483044611!5m2!1sen!2sbr'
                iframe.width = '350';
                iframe.height = '240';
                iframe.frameborder = '0';
                iframe.style = 'border:0';
                iframe.allowFullscreen = true;
                iframe.title = 'Nossa localização no Google Maps'

                maps.parentNode.replaceChild(iframe, maps)
            }, 400);
        }
    </script>
